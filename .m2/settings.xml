<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
        xmlns="http://maven.apache.org/SETTINGS/1.1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <mirrors>
        <mirror>
            <mirrorOf>*</mirrorOf>
            <name>repo</name>
            <url>http://artifactory-dev.uberinternal.com/artifactory/repo/</url>
            <id>repo</id>
        </mirror>
    </mirrors>

    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>Uber Release</name>
                    <url>http://artifactory-dev.uberinternal.com/artifactory/libs-release-local/</url>
                </repository>

                <repository>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <id>snapshots</id>
                    <name>Uber Snapshot</name>
                    <url>http://artifactory-dev.uberinternal.com/artifactory/libs-snapshot-local/</url>
                </repository>
            </repositories>

            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>Uber Plugins Release</name>
                    <url>http://artifactory-dev.uberinternal.com/artifactory/plugins-release-local/</url>
                </pluginRepository>

                <pluginRepository>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <id>snapshots</id>
                    <name>Uber Plugins Snapshot</name>
                    <url>http://artifactory-dev.uberinternal.com/artifactory/plugins-snapshot-local/</url>
                </pluginRepository>
            </pluginRepositories>

            <id>artifactory</id>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>